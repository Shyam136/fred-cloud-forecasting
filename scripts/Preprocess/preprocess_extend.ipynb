    "import pandas as pd\n",
    "\n",
    "# ------------------------------------------------------\n",
    "# 1. Load your merged dataset\n",
    "# ------------------------------------------------------\n",
    "path = r\"C:\\Users\\jesmi\\OneDrive\\Desktop\\0_cloud\\Project\\fred-cloud-forecasting\\data\\raw\\complete_economic_indicators.xlsx\"\n",
    "df = pd.read_excel(path)\n",
    "\n",
    "\n",
    "# ------------------------------------------------------\n",
    "# 2. Clean column names (remove spaces, uppercase/lowercase issues)\n",
    "# ------------------------------------------------------\n",
    "df.columns = df.columns.str.strip()\n",
    "\n",
    "print(\"Columns:\", df.columns.tolist())\n",
    "\n",
    "# ------------------------------------------------------\n",
    "# 3. Detect DATE column automatically\n",
    "# ------------------------------------------------------\n",
    "date_col = None\n",
    "for col in df.columns:\n",
    "    if col.lower() == \"date\":        # match DATE, Date, date, DATE \n",
    "        date_col = col\n",
    "        break\n",
    "\n",
    "# If still not found, check common issue: Unnamed: 0\n",
    "if date_col is None:\n",
    "    if \"Unnamed: 0\" in df.columns:\n",
    "        df = df.rename(columns={\"Unnamed: 0\": \"DATE\"})\n",
    "        date_col = \"DATE\"\n",
    "\n",
    "# If still not found → stop\n",
    "if date_col is None:\n",
    "    raise ValueError(\"No DATE column found in your dataframe.\")\n",
    "\n",
    "print(\"Detected DATE column:\", date_col)\n",
    "\n",
    "# ------------------------------------------------------\n",
    "# 4. Convert DATE column to datetime\n",
    "# ------------------------------------------------------\n",
    "df[date_col] = pd.to_datetime(df[date_col], errors=\"coerce\")\n",
    "\n",
    "# ------------------------------------------------------\n",
    "# 5. Set DATE as index\n",
    "# ------------------------------------------------------\n",
    "df = df.set_index(date_col)\n",
    "\n",
    "# ------------------------------------------------------\n",
    "# 6. Resample to monthly (M) and take mean\n",
    "# ------------------------------------------------------\n",
    "df = df.resample(\"ME\").mean()\n",
    "\n",
    "# ------------------------------------------------------\n",
    "# 7. Forward fill missing values\n",
    "# ------------------------------------------------------\n",
    "df = df.ffill()\n",
    "\n",
    "# ------------------------------------------------------\n",
    "# 8. Show results\n",
    "# ------------------------------------------------------\n",
    "print(\"After resampling:\", df.shape)\n",
    "display(df.head())\n"
   ]
  },
