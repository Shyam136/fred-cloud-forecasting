    "# ------------------------------------------------------\n",
    "# 12. Create master long dataset (optional)\n",
    "# ------------------------------------------------------\n",
    "\n",
    "# Ensure index has a proper name\n",
    "master_wide.index.name = \"DATE\"\n",
    "\n",
    "master_long = (\n",
    "    master_wide\n",
    "    .reset_index()\n",
    "    .melt(id_vars=\"DATE\", var_name=\"variable\", value_name=\"value\")\n",
    "    .sort_values([\"DATE\", \"variable\"])\n",
    ")\n",
    "\n",
    "print(\"Master long shape:\", master_long.shape)\n",
    "display(master_long.head())\n",
    "\n",
    "\n",
    "# ------------------------------------------------------\n",
    "# 13. Save outputs (LOCAL)\n",
    "# ------------------------------------------------------\n",
    "from pathlib import Path\n",
    "\n",
    "output_date = pd.Timestamp.today().strftime(\"%Y%m%d\")\n",
    "\n",
    "processed_dir = Path(\"data/processed\")\n",
    "processed_dir.mkdir(parents=True, exist_ok=True)\n",
    "\n",
    "wide_path = processed_dir / f\"master_wide_{output_date}.parquet\"\n",
    "long_path = processed_dir / f\"master_long_{output_date}.parquet\"\n",
    "\n",
    "master_wide.to_parquet(wide_path, index=True)\n",
    "master_long.to_parquet(long_path, index=False)\n",
    "\n",
    "print(\"Saved files:\")\n",
    "print(\"Wide  →\", wide_path)\n",
    "print(\"Long  →\", long_path)\n",
    "\n",
    "\n",
    "# ------------------------------------------------------\n",
    "# 14. Create metadata log\n",
    "# ------------------------------------------------------\n",
    "log_dir = Path(\"logs\")\n",
    "log_dir.mkdir(exist_ok=True)\n",
    "\n",
    "log_path = log_dir / f\"preprocess_{output_date}.md\"\n",
    "\n",
    "metadata_text = f\"\"\"\n",
    "# Preprocessing Log – {output_date}\n",
    "\n",
    "**Rows (wide)**: {master_wide.shape[0]}\n",
    "**Columns (wide)**: {master_wide.shape[1]}\n",
    "\n",
    "**Rows (long)**: {master_long.shape[0]}\n",
    "**Date Range**: {master_wide.index.min().date()} → {master_wide.index.max().date()}\n",
    "\n",
    "**Imputation**: Forward-fill monthly frequency alignment  \n",
    "**Resample Frequency**: Monthly end ('ME')  \n",
    "**Missing Columns Not Found**: {missing}\n",
    "\"\"\"\n",
    "\n",
    "with open(log_path, \"w\") as f:\n",
    "    f.write(metadata_text)\n",
    "\n",
    "print(\"Metadata log saved →\", log_path)\n"
